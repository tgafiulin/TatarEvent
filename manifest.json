{
  "name": "TatarEvent 2.0",
  "version": "1.1.0",
  "description": "Расширение для упрощения жизни в Jevent",
  "manifest_version": 3,
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["https://jevent.jugru.org/*"],
      "js": ["contentScript.js"],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "action": {
    "default_popup": "popup.html"
  }
}
